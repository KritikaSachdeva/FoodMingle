<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NPricedListOfItemsActivity"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    >


    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Activity.NPricedListOfItemsActivity"
        android:orientation="vertical"
        android:gravity="center">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">




            <android.support.v7.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="150dp"
                android:layout_height="200dp"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/c1"
                android:elevation="5dp"
                app:cardUseCompatPadding="true"
                android:layout_margin="4dp"
                android:layout_weight="1">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="180dp"
                        android:layout_gravity="center"
                        android:layout_height="150dp"
                        android:layout_marginTop="3dp"
                        android:src="@drawable/paobhaji"/>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="pav bhaji"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:textColor="#1e1e1e"
                        />






                </LinearLayout>




            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:layout_weight="1"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/c2"
                android:elevation="5dp"
                app:cardUseCompatPadding="true"
                android:layout_margin="4dp">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="180dp"
                        android:layout_gravity="center"
                        android:layout_height="150dp"
                        android:layout_marginTop="3dp"
                        android:src="@drawable/dal"/>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Maa Ki Dal"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:textColor="#1e1e1e"
                        />

                </LinearLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>








        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">




            <android.support.v7.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="150dp"
                android:layout_height="200dp"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/c3"
                android:elevation="5dp"
                app:cardUseCompatPadding="true"
                android:layout_margin="4dp"
                android:layout_weight="1">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="180dp"
                        android:layout_gravity="center"
                        android:layout_height="150dp"
                        android:layout_marginTop="3dp"
                        android:src="@drawable/chow"/>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Chowmein"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:textColor="#1e1e1e"
                        />

                </LinearLayout>

            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:layout_weight="1"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/c4"
                android:elevation="5dp"
                app:cardUseCompatPadding="true"
                android:layout_margin="4dp">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="180dp"
                        android:layout_gravity="center"
                        android:layout_height="150dp"
                        android:layout_marginTop="3dp"
                        android:src="@drawable/nandal"/>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Nan Dal"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:textColor="#1e1e1e"
                        />

                </LinearLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">




            <android.support.v7.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="150dp"
                android:layout_height="200dp"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/c5"
                android:elevation="5dp"
                app:cardUseCompatPadding="true"
                android:layout_margin="4dp"
                android:layout_weight="1">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="180dp"
                        android:layout_gravity="center"
                        android:layout_height="150dp"
                        android:layout_marginTop="3dp"
                        android:src="@drawable/gulab"/>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Gulab jamun"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:textColor="#1e1e1e"
                        />

                </LinearLayout>

            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:layout_weight="1"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/c6"
                android:elevation="5dp"
                app:cardUseCompatPadding="true"
                android:layout_margin="4dp">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="180dp"
                        android:layout_gravity="center"
                        android:layout_height="150dp"
                        android:layout_marginTop="3dp"
                        android:src="@drawable/dahibhalle"/>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Dahi Bhalle"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:textColor="#1e1e1e"
                        />

                </LinearLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>










    </LinearLayout>

</LinearLayout>